<?xml version="1.0" encoding="UTF-8"?>
<template xmlns="http://www.shareaza.com/schemas/Template.xsd" version="1.0">
<manifest	name="General Purple"
			author="CyberBob"
			version="0.1"
			link="http://peerproject.org/"
			email="mylene@plum.freemail.ne.jp"
